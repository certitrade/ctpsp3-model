{
 "name": "@payfunc/model",
 "version": "0.1.22",
 "description": "Data models for PayFunc.",
 "author": "PayFunc",
 "license": "MIT",
 "private": false,
 "main": "dist/index.js",
 "typings": "dist/index.d.ts",
 "browser": {
  "./dist/fetch": "./dist/fetch.browser"
 },
 "git": {
  "tagName": "v${version}"
 },
 "github": {
  "release": true
 },
 "jest": {
  "globals": {
   "ts-jest": {
    "tsConfig": "tsconfig.test.json"
   }
  },
  "testEnvironment": "node",
  "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
   "node_modules/",
   "dist/"
  ],
  "collectCoverageFrom": [
   "**/*.{ts,tsx,js,jsx}",
   "!**/node_modules/**",
   "!**/dist/**"
  ],
  "preset": "ts-jest",
  "testMatch": null
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "tslint --config tslint.json --project .",
  "build": "tsc -p .",
  "test": "jest",
  "test:watch": "watch jest",
  "prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "dependencies": {
  "@cardfunc/model": "0.0.32",
  "authly": "0.0.34",
  "dotenv": "^8.2.0",
  "gracely": "0.0.22",
  "isoly": "0.0.10",
  "node-fetch": "^2.6.0"
 },
 "devDependencies": {
  "@types/dotenv": "^8.2.0",
  "@types/jest": "^24.0.21",
  "@types/node": "^12.12.5",
  "@types/node-fetch": "^2.5.3",
  "babel-core": "^6.26.3",
  "babel-jest": "^24.9.0",
  "jest": "^24",
  "ts-jest": "^24.1.0",
  "tslint": "^5.20.0",
  "typescript": "^3.6.4",
  "watch": "^1.0.2"
 }
}
